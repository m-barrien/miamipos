    <sql>SELECT 	CAST&#x28; extract&#x28;year from venta.fecha&#x29; as integer&#x29; as year,&#xd;&#xa;		CAST&#x28;extract&#x28;month from venta.fecha&#x29; as integer&#x29; as month,&#xd;&#xa;		CAST&#x28;extract&#x28;day from venta.fecha&#x29; as integer&#x29; as day,&#xd;&#xa;		plu,&#xd;&#xa;		CAST&#x28;sum&#x28;venta_producto.cantidad&#x29; as integer&#x29; as cantidad,&#xd;&#xa;		CAST&#x28;sum&#x28;venta_producto.total&#x29; as integer&#x29; as total&#xd;&#xa;FROM venta_producto&#xd;&#xa;	,venta&#xd;&#xa;	,turno&#xd;&#xa;WHERE&#xd;&#xa;	venta_producto.id_venta &#x3d; venta.id_venta&#xd;&#xa;	AND venta.id_turno &#x3d; turno.id&#xd;&#xa;	AND turno.sucursal &#x3d; &#x24;&#x7b;LOCAL&#x7d;&#xd;&#xa;	AND extract&#x28;year from venta.fecha&#x29; &#x3d; &#x24;&#x7b;ANNO&#x7d;&#xd;&#xa;	AND extract&#x28;month from venta.fecha&#x29;&#x3d; &#x24;&#x7b;MES&#x7d;&#xd;&#xa;GROUP BY year, month , day ,plu&#xd;&#xa;ORDER BY plu</sql>
    <limit>0</limit>
